{
  "name": "starknet-box",
  "version": "1.0.0",
  "description": "Boilerplate proof of concept to compile, migrate, and perform L1 <-> L2 interactions on StarkNet.",
  "main": "truffle-config.starknet.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "",
    "installStarknetCli": "git clone -b main https://github.com/truffle-box/cairo-starknet-cli-docker.git",
    "buildStarknetCli": "cd cairo-starknet-cli-docker && docker build --build-arg CAIRO_VERSION=0.6.1 -t cairo-starknet-cli:0.6.1 .",
    "compile:starknet": "node ./scripts/compile_starknet.js",
    "deploy:starknet": "node ./scripts/deploy_starknet.js",
    "test:starknet": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/truffle-box/starknet-box.git"
  },
  "keywords": [
    "StarkNet",
    "Cairo",
    "Ethereum",
    "Truffle",
    "Layer",
    "2"
  ],
  "author": "David Killen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/truffle-box/starknet-box/issues"
  },
  "homepage": "https://github.com/truffle-box/starknet-box#readme",
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.4.1",
    "dotenv": "^10.0.0"
  },
  "dependencies": {}
}
