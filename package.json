{
  "name": "starknet-box",
  "version": "0.0.1",
  "description": "Boilerplate proof of concept to compile, migrate, and perform L1 <-> L2 interactions on StarkNet.",
  "main": "truffle-config.starknet.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "",
    "starknet:deploy_account": "node ./scripts/deploy_account.mjs --network=$npm_config_network",
    "starknet:compile": "node ./scripts/compile_starknet.mjs",
    "starknet:deploy": "node ./scripts/deploy_starknet.mjs --network=$npm_config_network",
    "starknet:test": "node ./scripts/test_starknet.mjs",
    "starknet:start_devnet": "node ./scripts/start_devnet.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/truffle-box/starknet-box.git"
  },
  "keywords": [
    "StarkNet",
    "Cairo",
    "Ethereum",
    "Truffle",
    "Layer",
    "2"
  ],
  "author": "David Killen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/truffle-box/starknet-box/issues"
  },
  "homepage": "https://github.com/truffle-box/starknet-box#readme",
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.4.1",
    "dotenv": "^10.0.0"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "dockerode": "^3.3.1",
    "fse": "^4.0.1",
    "node-fetch": "^2.6.7",
    "yargs": "^17.3.1"
  }
}
